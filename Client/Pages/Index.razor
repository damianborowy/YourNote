@page "/"

@for (int i = 1; i < 9; i++)
{
    <Note Title="Note title"
          Color="@i"
          Content=@GenerateContent() />
}

@code {
    public string GenerateContent()
    {
        String content = "lorem ipsum";
        String result = "";
        Random rng = new Random();

        int count = rng.Next(5, 50);

        for (int i = 0; i < count; ++i)
        {
            result += content;
        }

        Console.Write(result);
        return result;
    }
}